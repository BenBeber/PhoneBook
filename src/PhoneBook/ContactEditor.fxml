<DialogPane prefHeight="250.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PhoneBook.ContactEditorController">
   <graphic>
       <GridPane prefHeight="197.0" prefWidth="302.0">
           <columnConstraints>
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="262.0" minWidth="10.0" prefWidth="117.0"/>
               <ColumnConstraints hgrow="SOMETIMES" maxWidth="441.0" minWidth="10.0" prefWidth="185.0"/>
           </columnConstraints>
           <rowConstraints>
               <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
               <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" prefHeight="43.0" vgrow="SOMETIMES"/>
               <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" prefHeight="45.0" vgrow="SOMETIMES"/>
           </rowConstraints>
           <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  text="First name:" textAlignment="RIGHT">
              <font>
                  <Font size="14.0"/>
              </font>
           </Label>
           <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  text="Last Name:" GridPane.rowIndex="1">
              <font>
                  <Font size="14.0"/>
              </font>
           </Label>
           <Label alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                  prefHeight="26.0" prefWidth="186.0" text="Phone number:" GridPane.rowIndex="2">
              <font>
                  <Font size="14.0"/>
              </font>
           </Label>
           <TextField fx:id="firstNameField" GridPane.columnIndex="1"/>
           <TextField fx:id="lastNameField" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
           <TextField fx:id="phoneNumberField" GridPane.columnIndex="1" GridPane.rowIndex="2"/>
       </GridPane>
   </graphic>
</DialogPane>
